{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "preLaunchTask": "build",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/rivet.ts",
            "outFiles": [
                "${workspaceFolder}/dist/*.js"
            ],
            "env": {
                "OPEN_API_KEY": "Put your OpenAI API Key here!"
            }
        }
    ]
}